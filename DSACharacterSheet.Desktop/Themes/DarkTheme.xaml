    <ObjectDataProvider x:Key="BrushIcon" ObjectType="images:Dark" MethodName="get_Brush"/>
    <ObjectDataProvider x:Key="ClearIcon" ObjectType="images:Dark" MethodName="get_Clear"/>
    <ObjectDataProvider x:Key="EraserIcon" ObjectType="images:Dark" MethodName="get_Eraser"/>
    <ObjectDataProvider x:Key="PencilIcon" ObjectType="images:Dark" MethodName="get_Pencil"/>
    <ObjectDataProvider x:Key="RedoIcon" ObjectType="images:Dark" MethodName="get_Redo"/>
