    <ObjectDataProvider x:Key="ArrowBlockDownIcon" ObjectType="images:Dark" MethodName="get_ArrowBlockDown"/>

    <ObjectDataProvider x:Key="EditIcon" ObjectType="images:Dark" MethodName="get_EditIcon"/>
    <ObjectDataProvider x:Key="HtmlIcon" ObjectType="images:Dark" MethodName="get_HTML"/>
    <ObjectDataProvider x:Key="InfoIcon" ObjectType="images:Dark" MethodName="get_InfoIcon"/>
    <ObjectDataProvider x:Key="MenuIcon" ObjectType="images:Dark" MethodName="get_MenuIcon"/>
    <ObjectDataProvider x:Key="NewIcon" ObjectType="images:Dark" MethodName="get_NewIcon"/>
    <ObjectDataProvider x:Key="PdfIcon" ObjectType="images:Dark" MethodName="get_Pdf"/>
    <ObjectDataProvider x:Key="PrintIcon" ObjectType="images:Dark" MethodName="get_PrintIcon"/>
    <ObjectDataProvider x:Key="PropertiesIcon" ObjectType="images:Dark" MethodName="get_PropertiesIcon"/>
    <ObjectDataProvider x:Key="SaveAllIcon" ObjectType="images:Dark" MethodName="get_SaveAllIcon"/>
